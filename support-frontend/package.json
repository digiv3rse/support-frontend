    "chalk": "^4.1.0",
    "core-js": "^3.23.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.1.5",
    "eslint": "^7.29.0",
